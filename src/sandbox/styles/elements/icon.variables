@fontPath: '../../github/assets/fonts';
@src:
  url("~semantic-ui-less/themes/github/assets/fonts/@{fontName}.woff") format('woff'),
  url("~semantic-ui-less/themes/github/assets/fonts/@{fontName}.ttf") format('truetype'),
  url("~semantic-ui-less/themes/github/assets/fonts/@{fontName}.svg#icons") format('svg')
;